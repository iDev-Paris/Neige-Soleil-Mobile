<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Resultats">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="416dp"
        android:layout_height="164dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="26dp"
        app:layout_constraintBottom_toTopOf="@+id/idListe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ListView
        android:id="@+id/idListe"
        android:layout_width="411dp"
        android:layout_height="353dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="54dp"
        app:layout_constraintBottom_toTopOf="@+id/idRetour"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <Button
        android:id="@+id/idRetour"
        android:layout_width="410dp"
        android:layout_height="55dp"
        android:layout_marginTop="41dp"
        android:backgroundTint="#07588F"
        android:text="Retour"
        android:textColor="#FFF"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idListe" />
</androidx.constraintlayout.widget.ConstraintLayout>